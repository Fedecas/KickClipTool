{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$TEMP/*" }]
    },
    {
      "identifier": "fs:allow-temp-read-recursive",
      "allow": [{ "path": "$TEMP/*" }]
    }
  ]
}
